{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "Presets for GitHub actions",
  "extends": [
    "config:base",
    ":semanticCommits"
  ],
  "automerge": false,
  "automergeType": "branch",
  "packageRules": [
    {
      "matchManagers": [
        "github-actions"
      ],
      "reviewersFromCodeOwners": true,
      "enabled": true
    }
  ]
}